package week1.day1;

public class Fabonicseries {

	public static void main(String[] args) {
		int range= 8 ,firstNum= 0 ,secNum = 1 ,sum;
		System.out.println(firstNum);		
		System.out.println(secNum);
for (int i=1;i<7;++i)
	 {
		sum= firstNum+secNum;
		System.out.println(sum);
		firstNum=secNum;
		secNum = sum;
		
	 }}}
